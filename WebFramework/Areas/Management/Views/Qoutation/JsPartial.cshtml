<script>


    // resetAllFilter
    $('#filter .reset-button').click(function () {
        $('#filter input:not(:hidden)').val('');
        $('#filter').submit();
    });

    // reload table
    function reloadMainList() {
        $('#mainList').load('/Management/Qoutation/MainListPartial');
    }

    function deleteQoutation(qoutationId) {
        $.post('/Management/Qoutation/DeleteQoutation?qoutationId=' + qoutationId).done(function () {
            window.location.reload();
        });
    }

    function onInputSubmitFail(response) {
        console.log(response.responseText);
    }
</script>